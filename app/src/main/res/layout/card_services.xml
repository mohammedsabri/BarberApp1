                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="195dp">

                        <ImageView
                            android:id="@+id/imageView1"
                            android:layout_width="match_parent"
                            android:layout_height="195dp"
                            android:background="@drawable/skinfade"
                            android:scaleType="matrix"
                            tools:ignore="DuplicateIds" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="15dp"
                            android:layout_marginTop="15dp"
                            android:layout_marginEnd="15dp"
                            android:layout_marginRight="17dp"
                            android:layout_marginBottom="14dp"
                            android:background="@drawable/relativelayout_background"
                            android:paddingLeft="12dp"
                            android:paddingTop="3dp"
                            android:paddingRight="12dp"
                            android:paddingBottom="3dp">

                            <ImageView
                                android:id="@+id/imageViewLike1"
                                android:layout_width="22dp"
                                android:layout_height="22dp"
                                android:layout_marginTop="3dp"
                                android:layout_marginRight="6dp"
                                android:background="@drawable/ic_like"
                                tools:ignore="DuplicateIds" />



                        </RelativeLayout>

                    </RelativeLayout>





                    <RelativeLayout
                        android:padding="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/textViewTitle2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Skin Fade"
                            android:textColor="#1A1A1A"
                            android:textSize="18sp" />

                        <TextView
                            android:id="@+id/textViewPrice5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/textViewTitle2"
                            android:text=" €20"
                            tools:ignore="DuplicateIds" />

                        <Button
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="Book" />

                    </RelativeLayout>


        </LinearLayout>
    </androidx.cardview.widget.CardView>











